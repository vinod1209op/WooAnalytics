		"api:dev": "pnpm --filter './apps/api' run dev"
	},
	"devDependencies": {
		"@types/node": "^24.10.1",
		"autoprefixer": "^10.4.22",
		"postcss": "^8.5.6",
		"prisma": "^6.19.0",
		"shadcn": "^3.5.0",
		"tailwindcss": "^4.1.17"
	},
	"dependencies": {
		"@prisma/client": "^6.19.0",
		"@upstash/redis": "^1.35.6"
	}
}

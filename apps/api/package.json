{
  "name": "@wooanalytics/api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "echo \"skipping tsc build\"",
    "start": "ts-node --transpile-only src/index.ts",
    "prisma:generate": "prisma generate --schema ../../prisma/schema.prisma",
    "postinstall": "pnpm prisma:generate",
    "snapshot:notion": "ts-node --transpile-only src/scripts/push-kpi-snapshot.ts",
    "test:customers": "ts-node --transpile-only src/tests/customers.integration.ts"
  },
  "dependencies": {
    "@notionhq/client": "^5.4.0",
    "@prisma/client": "^6.19.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.19.0",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "openai": "^4.67.2",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/supertest": "^2.0.16",
    "@types/node": "^20.0.0",
    "prisma": "^6.19.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.0",
    "supertest": "^6.3.4"
  }
}

DATABASE_URL="postgresql://USER:PASSWORD@HOST:PORT/DATABASE"
# Optional: change if 3001 is busy locally
PORT=3001

# Optional: cron auth
# CRON_SECRET=your_secret

# Optional: GoHighLevel integration
# GHL_PIT=your_private_integration_token
# GHL_LOCATION_ID=your_location_id
# GHL_API_BASE=https://services.leadconnectorhq.com

# Optional: tune live GHL dashboard pull
# GHL_DASHBOARD_MSG_PAGE_LIMIT=200
# GHL_DASHBOARD_MAX_PAGES=25

# Optional: workflow analytics event store + AI analysis model
# GHL_WORKFLOW_EVENTS_FILE=apps/api/data/ghl-workflow-events.json
# GHL_AI_ANALYSIS_MODEL=openai/gpt-4o-mini
# GHL_WORKFLOW_WEBHOOK_SECRET=replace_with_long_random_secret

# Optional: persist AI analysis outputs into GHL contact custom fields
# GHL_AI_INTENT_FIELD_ID=custom_field_id_for_intent
# GHL_AI_SENTIMENT_FIELD_ID=custom_field_id_for_sentiment
# GHL_AI_LEAD_TEMPERATURE_FIELD_ID=custom_field_id_for_lead_temperature
# GHL_AI_ENGAGED_FIELD_ID=custom_field_id_for_engaged
# GHL_AI_BOOKED_FIELD_ID=custom_field_id_for_booked
# GHL_AI_SUMMARY_FIELD_ID=custom_field_id_for_ai_summary
# GHL_AI_RECOMMENDED_ACTION_FIELD_ID=custom_field_id_for_recommended_action

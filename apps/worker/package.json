{
  "name": "@wooanalytics/worker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "prisma:generate": "prisma generate --schema ../../prisma/schema.prisma",
    "seed:store": "ts-node src/scripts/seed-store.ts",
    "postinstall": "pnpm prisma:generate",
    "sync:full": "ts-node src/scripts/run-full-sync.ts",
    "sync:since": "ts-node src/scripts/run-since-sync.ts",
    "sync:coupons": "ts-node src/scripts/run-coupon-sync.ts"
  },
  "dependencies": {
    "@notionhq/client": "^5.4.0",
    "@prisma/client": "^6.19.0",
    "axios": "^1.13.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "inngest": "^3.45.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.25",
    "@vercel/node": "^5.5.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}
